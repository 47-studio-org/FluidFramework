{
    "extends": "nx/presets/npm.json",
    "npmScope": "monorepo",
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/nx-cloud",
            "options": {
                "useDaemonProcess": true,
                "parallel": 7,
                "cacheableOperations": [
                    "build:commonjs",
                    "build:compile",
                    "build:docs",
                    "build:esnext",
                    "build:test",
                    "build:test:jest",
                    "build:test:mocha",
                    "build:test:types",
                    "build:full",
                    "build",
                    "eslint",
                    "build:gen",
                    "build:genver",
                    "lint",
                    "package",
                    "prepare",
                    "test",
                    "tsc"
                ],
                "accessToken": "ZGRiNDM3YmMtMWZmZC00Yzc3LWExZTEtNjc5NGMyZTYxOTI0fHJlYWQtd3JpdGU="
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ],
        "build:full": [
            {
                "target": "build:full",
                "projects": "dependencies"
            }
        ],
        "build:commonjs": [
            {
                "target": "build:commonjs",
                "projects": "dependencies"
            }
        ],
        "build:compile": [
            {
                "target": "build:compile",
                "projects": "dependencies"
            }
        ],
        "build:esnext": [
            {
                "target": "build:esnext",
                "projects": "dependencies"
            }
        ],
        "build:docs": [
            {
                "target": "tsc",
                "projects": "dependencies"
            },
            {
                "target": "tsc",
                "projects": "self"
            }
        ],
        "build:test": [
            {
                "target": "tsc",
                "projects": "dependencies"
            },
            {
                "target": "tsc",
                "projects": "self"
            },
            {
                "target": "build:test",
                "projects": "dependencies"
            }
        ],
        "build:test:jest": [
            {
                "target": "tsc",
                "projects": "dependencies"
            },
            {
                "target": "tsc",
                "projects": "self"
            }
        ],
        "build:test:mocha": [
            {
                "target": "tsc",
                "projects": "dependencies"
            },
            {
                "target": "tsc",
                "projects": "self"
            }
        ],
        "build:test:types": [
            {
                "target": "tsc",
                "projects": "dependencies"
            },
            {
                "target": "tsc",
                "projects": "self"
            }
        ],
        "tsc": [
            {
                "target": "tsc",
                "projects": "dependencies"
            }
        ],
        "lint": [
        ],
        "test": [
            {
                "target": "build:test",
                "projects": "dependencies"
            }
        ],
        "prepare": [
            {
                "target": "prepare",
                "projects": "dependencies"
            }
        ],
        "package": [
            {
                "target": "package",
                "projects": "dependencies"
            }
        ]
    },
    "affected": {
        "defaultBase": "yarn"
    },
    "pluginsConfig": {
        "@nrwl/js": {
            "analyzeSourceFiles": false
        }
    }
}
